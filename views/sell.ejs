  <form action="/api/items" method="post" id="new-item-form" class="new-item-form">
    <div class="new-item-form__field-wrapper">
      <label for="new-item-form__title">Title</label>
      <input type="text" name="title" placeholder="Title" id="new-item-form__title">
    </div>

    <div class="new-item-form__field-wrapper">
      <label for="new-item-form__description">Description</label>
      <textarea placeholder="Description" name="description" id="item-form__description" cols="30" rows="5"></textarea>
    </div>


    <div class="new-item-form__field-wrapper">
      <label for="new-item-form__cost">Price</label>
      <input placeholder="Price" type="number" name="price" id="new_item_form__price">
    </div>

    <div class="new-item-form__field-wrapper">
      <label for="new-item-form__image">Image</label>
      <input placeholder="Image" type="text" name="item_photo_url" id="new-item-form__image">
    </div>


      <div class="new-item-form__field-wrapper">
          <button>Create</button>
          <a id="item-form__cancel" href="#">Cancel</a>
      </div>

